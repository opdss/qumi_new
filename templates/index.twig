<!DOCTYPE html>
<html lang="zh-CN">
<head>
    <meta charset="utf-8">
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="format-detection" content="telephone=no">
    <meta name="author" content="{{ author|default(site.author) }}">
    <meta name="keyword" content="{{ keyword|default(site.keyword) }}">
    <meta name="description" content="{{ description|default(site.description) }}">
    <title>{{ title|default(site.title) }}</title>

    <link rel="stylesheet" href="/statics/layui/css/layui.css">
    <link rel="stylesheet" href="/statics/css/index.css">

    {#每个页面额外载入的css文件#}
    {% for css in statics.css %}
        <link href="{{ css }}" rel="stylesheet">
    {% endfor %}

    {#每个页面额外添加的css代码#}
    {% block css %}
        {# 额外添加的css #}
    {% endblock %}
    {{ site.tongji|raw }}
</head>

<body>
{% block navbar %}
    <!-- header start -->
    <div class="layui-header header" autumn style="background-color: ">
        <div class="layui-main">
            <a class="logo layui-left" href="/">
                <img src="/statics/images/logo.jpg" title="趣米">
            </a>
            {#<div class="layui-form component" lay-filter="LAY-site-header-component"></div>#}
            <ul class="layui-nav layui-left layui-bg-white layui-hide-xs">
                <li class="layui-nav-item layui-this">
                    <a href="/">首页</a>
                </li>
                <li class="layui-nav-item">
                    <a href="{{ path_for('index.domains') }}">域名列表</a>
                </li>
                <li class="layui-nav-item">
                    <a href="{{ path_for('article.index') }}">米市资讯</a>
                </li>
                <li class="layui-nav-item">
                    <a href="https://qumi.app/article/1">APP趣站</a>
                </li>
                <li class="layui-nav-item">
                    <a href="https://qumi.app/article/1">我的域名</a>
                </li>
                <li class="layui-nav-item">
                    <a href="{{ path_for('home.feedback.index') }}">留言建议<span class="layui-badge-dot"></span></a>
                </li>
            </ul>
            <ul class="layui-nav layui-right layui-bg-white">
                {% if userInfo %}
                    <li class="layui-nav-item">
                        <a href="javascript:;">{{ userInfo.email }}</a>
                        <dl class="layui-nav-child">
                            <dd class="layui-hide-sm layui-show-xs"><a href="{{ path_for('index.domains') }}" target="_blank">域名列表</a></dd>
                            <dd class="layui-hide-sm layui-show-xs"><a href="{{ path_for('article.index') }}" target="_blank">米市资讯</a></dd>
                            <dd class="layui-hide-sm layui-show-xs"><a href="/" target="_blank">我的域名</a></dd>
                            <hr>
                            {% for item in menus %}
                                <dd><a href="{{ item.url }}">{{ item.name }}</a></dd>
                            {% endfor %}
                            <hr/>
                            <dd><a href="{{ path_for('logout') }}">安全退出</a></dd>
                        </dl>
                    </li>
                {% else %}
                    <li class="layui-nav-item">
                        <a href="{{ path_for('login') }}">登陆</a>
                    </li>
                    <li class="layui-nav-item">
                        <a href="{{ path_for('register') }}">注册</a>
                    </li>
                {% endif %}
            </ul>
        </div>
    </div>
